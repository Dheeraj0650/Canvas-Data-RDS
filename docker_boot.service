[Unit]
Description=docker boot
After=docker.service

[Service]
Type=oneshot
RemainAfterExit=yes
WorkingDirectory=/var/lib/jenkins/workspace/Alt\ Text\ Backend
ExecStart=/usr/bin/docker-compose -f /var/lib/jenkins/workspace/Alt\ Text\ Backend/docker-compose.yml up -d --remove-orphans

[Install]
WantedBy=multi-user.target